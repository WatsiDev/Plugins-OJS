<?xml version="1.0" encoding="UTF-8"?>

<!-- Define el tipo de documento como una definición de tipo de documento (DTD) para la versión del plugin -->
<!DOCTYPE version SYSTEM "../../../lib/pkp/dtd/pluginVersion.dtd">

<!-- Elemento raíz que contiene la información de la versión del plugin -->
<version>
    <!-- Nombre de la aplicación a la que pertenece el plugin -->
    <application>PlayerAudio</application>
    
    <!-- Tipo de plugin, en este caso es un plugin genérico -->
    <type>plugins.generic</type>
    
    <!-- Versión de lanzamiento del plugin -->
    <release>1.0.0.0</release>
    
    <!-- Fecha de lanzamiento del plugin -->
    <date>2024-06-20</date>
    
    <!-- Indica si el plugin se carga de manera perezosa (lazy-loading) -->
    <lazy-load>1</lazy-load>
    
    <!-- Clase principal del plugin -->
    <class>PlayerAudio</class>
</version>
